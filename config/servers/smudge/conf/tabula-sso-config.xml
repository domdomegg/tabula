<?xml version="1.0" encoding="UTF-8"?>
<config>
	<httpbasic><allow>true</allow></httpbasic>
	<mode>new</mode>

	<cluster>
		<enabled>true</enabled>
		<datasource>java:comp/env/jdbc/TabulaDS</datasource>
	</cluster>

	<shire>
		<filteruserkey>SSO_USER</filteruserkey>
		<uri-header>x-requested-uri</uri-header>
		<location>https://smudge.warwick.ac.uk/sso/acs</location>
		<sscookie>
			<name>SSO-SSC-Tabula</name>
			<path>/</path>
			<domain>smudge.warwick.ac.uk</domain>
		</sscookie>
		<providerid>urn:smudge.warwick.ac.uk:courses:service</providerid>
	</shire>

	<credentials>
		<certificate>file:/apps/apache/conf/ssl/2015/smudge.warwick.ac.uk.crt</certificate>
		<key>file:/apps/apache/conf/ssl/2015/smudge.warwick.ac.uk.key</key>
		<chain>file:/apps/apache/conf/ssl/2015/terena-sha2-ca.crt</chain>
	</credentials>

	<trustedapps>
		<publickey></publickey>
		<privatekey></privatekey>
	</trustedapps>

</config>
